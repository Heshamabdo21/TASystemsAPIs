{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/Welcome2",
    "definitions": {
        "Welcome2": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "page": {
                    "type": "integer"
                },
                "size": {
                    "type": "integer"
                },
                "totalPages": {
                    "type": "integer"
                },
                "totalElements": {
                    "type": "integer"
                },
                "content": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Content"
                    }
                }
            },
            "required": [
                "page",
                "size",
                "totalPages",
                "totalElements",
                "content"
            ],
            "title": "Welcome2"
        },
        "Content": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "id": {
                    "type": "integer"
                },
                "seasonType": {
                    "$ref": "#/definitions/SeasonType"
                },
                "saleSystem": {
                    "$ref": "#/definitions/SaleSystem"
                },
                "programType": {
                    "$ref": "#/definitions/ProgramType"
                },
                "programMechanism": {
                    "$ref": "#/definitions/ProgramMechanism"
                },
                "routeId": {
                    "type": "integer"
                },
                "season": {
                    "type": "integer"
                },
                "vehicleTypeId": {
                    "type": "integer"
                },
                "vehicleCategoryId": {
                    "type": "integer"
                },
                "minimumModelYear": {
                    "type": "integer"
                },
                "maximumModelYear": {
                    "type": "integer"
                },
                "minimumVehiclePricePer": {
                    "type": "integer"
                },
                "minimumSeat": {
                    "type": "integer"
                },
                "maximumSeat": {
                    "type": "integer"
                },
                "templatePeriodStartDate": {
                    "type": "string",
                    "format": "date-time"
                },
                "templatePeriodEndDate": {
                    "type": "string",
                    "format": "date-time"
                },
                "templatePeriodMaximumQuota": {
                    "type": "integer"
                },
                "isActive": {
                    "type": "boolean"
                }
            },
            "required": [
                "id",
                "isActive",
                "maximumModelYear",
                "maximumSeat",
                "minimumModelYear",
                "minimumSeat",
                "minimumVehiclePricePer",
                "programMechanism",
                "programType",
                "routeId",
                "saleSystem",
                "season",
                "seasonType",
                "templatePeriodEndDate",
                "templatePeriodMaximumQuota",
                "templatePeriodStartDate",
                "vehicleCategoryId",
                "vehicleTypeId"
            ],
            "title": "Content"
        },
        "ProgramMechanism": {
            "type": "string",
            "enum": [
                "reservations"
            ],
            "title": "ProgramMechanism"
        },
        "ProgramType": {
            "type": "string",
            "enum": [
                "Routes"
            ],
            "title": "ProgramType"
        },
        "SaleSystem": {
            "type": "string",
            "enum": [
                "GDS"
            ],
            "title": "SaleSystem"
        },
        "SeasonType": {
            "type": "string",
            "enum": [
                "umrah"
            ],
            "title": "SeasonType"
        }
    }
}
