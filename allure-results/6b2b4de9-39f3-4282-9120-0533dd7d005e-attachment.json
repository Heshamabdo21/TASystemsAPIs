{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/Welcome2",
  "definitions": {
    "Policy": {
      "additionalProperties": false,
      "type": "object",
      "title": "Policy",
      "properties": {
        "descriptionArabic": {
          "type": "string"
        },
        "nameArabic": {
          "type": "string"
        },
        "nameEnglish": {
          "type": "string"
        },
        "chargeType": {
          "$ref": "#/definitions/ChargeType"
        },
        "id": {
          "type": "integer"
        },
        "isActive": {
          "type": "boolean"
        },
        "deadline": {
          "type": "integer"
        },
        "chargeValue": {
          "type": "number"
        },
        "descriptionEnglish": {
          "type": "string"
        },
        "chargeUnit": {
          "$ref": "#/definitions/ChargeUnit"
        }
      },
      "required": [
        "descriptionArabic",
        "descriptionEnglish",
        "id",
        "isActive",
        "nameArabic",
        "nameEnglish"
      ]
    },
    "GeneralPolicy": {
      "additionalProperties": false,
      "type": "object",
      "title": "GeneralPolicy",
      "properties": {
        "descriptionArabic": {
          "type": "string"
        },
        "nameArabic": {
          "type": "string"
        },
        "nameEnglish": {
          "type": "string"
        },
        "id": {
          "type": "integer"
        },
        "isActive": {
          "type": "boolean"
        },
        "descriptionEnglish": {
          "type": "string"
        }
      },
      "required": [
        "descriptionArabic",
        "descriptionEnglish",
        "id",
        "isActive",
        "nameArabic",
        "nameEnglish"
      ]
    },
    "ChargeUnit": {
      "type": "string",
      "title": "ChargeUnit",
      "enum": [
        "days"
      ]
    },
    "ChargeType": {
      "type": "string",
      "title": "ChargeType",
      "enum": [
        "percentage"
      ]
    },
    "PaymentPolicy": {
      "additionalProperties": false,
      "type": "object",
      "title": "PaymentPolicy",
      "properties": {
        "descriptionArabic": {
          "type": "string"
        },
        "refundType": {
          "type": "string"
        },
        "nameArabic": {
          "type": "string"
        },
        "nameEnglish": {
          "type": "string"
        },
        "cancellationPolicyId": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ]
        },
        "id": {
          "type": "integer"
        },
        "isActive": {
          "type": "boolean"
        },
        "descriptionEnglish": {
          "type": "string"
        },
        "isRefundable": {
          "type": "boolean"
        }
      },
      "required": [
        "cancellationPolicyId",
        "descriptionArabic",
        "descriptionEnglish",
        "id",
        "isActive",
        "isRefundable",
        "nameArabic",
        "nameEnglish",
        "refundType"
      ]
    },
    "Welcome2": {
      "additionalProperties": false,
      "type": "object",
      "title": "Welcome2",
      "properties": {
        "generalPolicies": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/GeneralPolicy"
          }
        },
        "taxesPolicies": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Policy"
          }
        },
        "usagePolicies": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Policy"
          }
        },
        "cancellationPolicies": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Policy"
          }
        },
        "paymentPolicies": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/PaymentPolicy"
          }
        }
      },
      "required": [
        "cancellationPolicies",
        "generalPolicies",
        "paymentPolicies",
        "taxesPolicies",
        "usagePolicies"
      ]
    }
  }
}